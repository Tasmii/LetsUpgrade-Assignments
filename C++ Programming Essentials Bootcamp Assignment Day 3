#include <iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"enter value of a: ";
    cin>>a;

    cout<<"enter value of b: ";
    cin>>b;
    if(a>b){
        cout<<"a is greater "<<endl;
    }
    else if(a==b){
        cout<< "a is equal to b"<<endl;
    }
    else{
        cout<< "b is greater" << endl;
    }
        return 0;
}
